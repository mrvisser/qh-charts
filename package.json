    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-deprecation": "^1.2.1",
